.wrapper {
  padding: 0 10px;
  background-color: #fff;
  border-bottom: 1px solid #eee;

  display: flex;
  height: 60px;
  align-items: center;
}

.logo {
  padding: 0 15px;
}

.links {
  & > a {
    font-weight: 400;
    padding: 0 10px;
    text-decoration: none;
    color: #333333;

    &:hover {
      font-weight: 600;
    }

    &.active {
      font-weight: 600;
      border-bottom: 3px solid;
      color: #642891;
      cursor: default;
      pointer-events: none;
    }
  }
}
